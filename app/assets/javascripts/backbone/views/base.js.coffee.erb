        TweenMax.to @sidebar, 0.35,
          css:
            marginRight: 0
            opacity: 1
          ease: Circ.easeInOut
          delay: 0
        TweenMax.to $('.sidebar .sidebar-footer'), 0.35,
          css:
            right: 0
            opacity: 1
          ease: Circ.easeInOut
          delay: 0
        return
      $('html').on 'mouseleave', 'body.rtl.sidebar-hover .sidebar', ->
        if @body.hasClass('sidebar-condensed')
          TweenMax.to @sidebar, 0.35,
            css:
              marginRight: -170
              opacity: 0
            ease: Circ.easeInOut
            delay: 0
          TweenMax.to $('.sidebar .sidebar-footer'), 0.35,
            css:
              right: -170
              opacity: 0
            ease: Circ.easeInOut
            delay: 0
        else if @body.hasClass('sidebar-light')

    @createSideScroll();
    @toggleSidebarMenu();
    @customScroll();
    @handleSidebarSortable();
    @sidebarWidgets();
    @reposition_topnav();
    @handleSidebarRemove();
    @handleSidebarHide();
    @changeUserStatut();
    @handlePanelAction();
    @scrollTop();
    @sidebarBehaviour();
    @detectIE();
    @quickviewSidebar();
    @quickviewHeight();

    if $('body').hasClass('sidebar-hover')

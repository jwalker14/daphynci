            css:
              marginRight: -220
              opacity: 0
            ease: Circ.easeInOut
            delay: 0
          TweenMax.to $('.sidebar .sidebar-footer'), 0.35,
            css:
              right: -220
              opacity: 0
            ease: Circ.easeInOut
            delay: 0
        else
          TweenMax.to @sidebar, 0.35,
            css:
              marginRight: -220
              opacity: 0
            ease: Circ.easeInOut
            delay: 0
          TweenMax.to $('.sidebar .sidebar-footer'), 0.35,
            css:
              right: -220
              opacity: 0
            ease: Circ.easeInOut
            delay: 0
        return
    if !$('body').hasClass('rtl')

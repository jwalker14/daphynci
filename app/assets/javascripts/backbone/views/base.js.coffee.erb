  ###***  Variables Initiation  ***###



  ###*** Color Picker ***###

  colorPicker: ->
    if $('.color-picker').length and $.fn.spectrum
      $('.color-picker').each ->
        current_palette = ''
        if $(this).data('palette')
          current_palette = $(this).data('palette')
        $(this).spectrum
          color: if $(this).data('min') then $(this).data('min') else '#D15ADE'
          showInput: if $(this).data('show-input') then $(this).data('show-input') else false
          showPalette: if $(this).data('show-palette') then $(this).data('show-palette') else false
          showPaletteOnly: if $(this).data('show-palette-only') then $(this).data('show-palette-only') else false
          showAlpha: if $(this).data('show-alpha') then $(this).data('show-alpha') else false
          palette: if $(this).data('palette') then $(this).data('palette') else [ [ current_palette ] ]
        $(this).show()
        return
    return

  ###*** Numeric Stepper ***###

  numericStepper: ->
    if $('.numeric-stepper').length and $.fn.TouchSpin
      $('.numeric-stepper').each ->
        $(this).TouchSpin
          min: if $(this).data('min') then $(this).data('min') else 0
          max: if $(this).data('max') then $(this).data('max') else 100
          step: if $(this).data('step') then $(this).data('step') else 0.1
          decimals: if $(this).data('decimals') then $(this).data('decimals') else 0
          boostat: if $(this).data('boostat') then $(this).data('boostat') else 5
          maxboostedstep: if $(this).data('maxboostedstep') then $(this).data('maxboostedstep') else 10
          verticalbuttons: if $(this).data('vertical') then $(this).data('vertical') else false
          buttondown_class: if $(this).data('btn-before') then 'btn btn-' + $(this).data('btn-before') else 'btn btn-default'
          buttonup_class: if $(this).data('btn-after') then 'btn btn-' + $(this).data('btn-after') else 'btn btn-default'
        return
    return

  ###*** Sortable Portlets ***###

  sortablePortlets = ->
    if $('.portlets').length and $.fn.sortable
      $('.portlets').sortable
        connectWith: '.portlets'
        handle: '.panel-header'
        items: 'div.panel'
        placeholder: 'panel-placeholder'
        opacity: 0.5
        dropOnEmpty: true
        forcePlaceholderSize: true
        receive: (event, ui) ->
          $('body').trigger 'resize'
          return
    return

  hideLoader: ->
    'use strict'
    setTimeout (->
      $('.loader-overlay').addClass 'loaded'
      $('body > section').animate { opacity: 1 }, 400
      return
    ), 500
    return

  ###*** Nestable List ***###

  nestable: ->
    if $('.nestable').length and $.fn.nestable
      $('.nestable').nestable()
    return

  ###*** Sortable Table ***###

  sortableTable: ->
    if $('.sortable_table').length and $.fn.sortable
      $('.sortable_table').sortable
        itemPath: '> tbody'
        itemSelector: 'tbody tr'
        placeholder: '<tr class="placeholder"/>'
    return

  ###***  Show Tooltip  ***###

  showTooltip: ->
    if $('[data-rel="tooltip"]').length and $.fn.tooltip
      $('[data-rel="tooltip"]').tooltip()
    return

  ###***  Show Popover  ***###

  popover: ->
    if $('[rel="popover"]').length and $.fn.popover
      $('[rel="popover"]').popover trigger: 'hover'
      $('[rel="popover_dark"]').popover
        template: '<div class="popover popover-dark"><div class="arrow"></div><h3 class="popover-title popover-title"></h3><div class="popover-content popover-content"></div></div>'
        trigger: 'hover'
    return

  ###***  Table progress bar  ***###

  progressBar: ->
    if $('.progress-bar').length and $.fn.progressbar
      $('.progress-bar').progressbar()
    return

